<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BusinessCardView">
        <attr name="title" format="string" />
        <attr name="actionBtnTitle" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleStyle" format="enum">
            <enum name="bold" value="0" />
            <enum name="italic" value="1" />
            <enum name="normal" value="2" />
        </attr>
        <attr name="businessCardImage" format="string" />
        <attr name="actionBtnColor" format="color" />
    </declare-styleable>
</resources>